profiles {
    uw_batch {
        workDir = 's3://uwlm-nextflow-data/workdir/ncgl-nf'
        process {
            scratch = "/docker_scratch"
            queue = 'ngs-queue'
            executor = 'awsbatch'
            errorStrategy = 'ignore'
            withLabel:pipeline {
                cpus = 30
                memory = '60GB'
            }
        }
        aws {
            region = 'us-west-2'
            batch {
                volumes = '/docker_scratch'
                cliPath = '/home/ec2-user/miniconda/bin/aws'
            }
        }
    }
}